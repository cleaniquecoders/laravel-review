{
    "name": "cleaniquecoders/laravel-review",
    "description": "Request for Review with Laravel Review",
    "keywords": ["package", "laravel", "review", "requestor", "reviewer", "approval", "reject"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\LaravelReview\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\LaravelReview\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "cleaniquecoders/blueprint-macro": "dev-master#3bcce25b1699433862565c8c594bd4c1bd315086",
        "illuminate/support": "^5.5|^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\LaravelReview\\LaravelReviewServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
